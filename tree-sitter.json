  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bibtex",
      "scope": "source.bibtex",
      "file-types": [ "bib" ],
      "injection-regex": "^bibtex$"
    }
  ],
  "metadata": {
