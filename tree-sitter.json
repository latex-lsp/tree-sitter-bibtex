    "license": "MIT",
    "description": "BibTeX grammar for tree-sitter",
    "authors": [
      {
        "name": "Patrick FÃ¶rster",
        "email": "patrick.foerster@outlook.de"
      }
    ],
    "links": {
      "repository": "https://github.com/latex-lsp/tree-sitter-bibtex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
